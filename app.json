{
  "name": "GateKeeper",
  "description": "Enables client-side applications to dance OAuth with GitHub or GitLab.",
  "website": "https://github.com/HollowMan6/GateKeeper",
  "repository": "https://github.com/HollowMan6/GateKeeper",
  "keywords": ["node", "github", "oauth"],
  "env": {
    "OAUTH_CLIENT_ID": {"required": true},
    "OAUTH_CLIENT_SECRET": {"required": true},
    "TELEGRAM_BOT_TOKEN": {"required": false},
    "TELEGRAM_CHAT_ID": {"required": false},
    "REDIRECT_URL": {"required": false},
    "DELIVERY_SECRET": {"required": false},
    "OAUTH_HOST": "github.com",
    "OAUTH_PORT": "443",
    "OAUTH_PATH": "/login/oauth/access_token",
    "OAUTH_METHOD": "POST"
  }
}
